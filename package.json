  "dependencies": {
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "@fancyapps/fancybox": "^3.5.2",
    "@fortawesome/fontawesome-free": "^5.4.1",
    "aos": "^2.2.0",
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.1.0",
    "browser-sync": "^2.26.3",
    "cpx": "^1.5.0",
    "gsap": "^1.20.3",
    "imagemin-cli": "^3.0.0",
    "imagesloaded": "^4.1.3",
    "imports-loader": "^0.7.1",
    "isotope-layout": "^3.0.6",
    "jquery": "^3.2.1",
    "jshint": "^2.9.5",
    "lazysizes": "^4.0.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "onchange": "^3.2.1",
    "parallax-js": "^3.1.0",
    "popper.js": "^1.12.3",
    "postcss-cli": "^4.1.1",
    "rimraf": "^2.6.2",
    "scrollmagic": "^2.0.5",
    "scrollup": "^1.0.1",
    "slick-carousel": "^1.8.1",
    "smoothscroll-for-websites": "^1.4.6",
    "tippy.js": "^3.0.6",
    "webpack": "^3.8.1"
  },
  "scripts": {
    "clean": "rimraf dist/{css/*,js/*,images/*}",
    "autoprefixer": "postcss -u autoprefixer -r dist/css/*",
    "scss": "node-sass --output-style compressed -o dist/css src/scss/styles.scss",
    "webpack": "webpack",
    "imagemin": "imagemin src/images/**/* -o dist/images",
    "serve": "browser-sync dist",
    "jekyll": "bundle exec jekyll build --watch",
    "build:css": "run-s scss autoprefixer",
    "build:js": "webpack",
    "build:images": "run-s imagemin",
    "build": "run-s clean build:*",
    "watch:css": "onchange \"src/scss\" -- run-s build:css",
    "watch:js": "onchange \"src/js/*.js\" -- run-s build:js",
    "watch:images": "onchange \"src/images/*\" -- run-s build:images",
    "watch": "run-p jekyll serve watch:*",
    "start": "run-s build watch",
    "h": "browser-sync --version"
  }
}
